                  <div class="col-sm-4">
                    <div class="w-like">
							<!-- Button trigger modal -->
							<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleeight" data-whatever="@eighth">+</button>

							<!-- Modal -->
							<div class="modal fade" id="exampleeight" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitleEight" aria-hidden="true">
  								<div class="modal-dialog modal-lg modal-dialog-centered" role="document">
    								<div class="modal-content">
    								  <div class="modal-header">
        								<h5 class="modal-title" id="exampleModalLongTitleEight">CS 3160</h5>
        								<button type="button" class="close" data-dismiss="modal" aria-label="Close">
